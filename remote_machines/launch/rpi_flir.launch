<launch>

 <machine
   name="rpi2"
   address="rpi2"
   user="pandora"
   timeout="20.0"
   env-loader="~/pandora_ws/src/rpi_hardware_interface/scripts/raspberry_env.sh" />

 <!-- Flir Lepton -->
 <node
   machine="rpi2"
   name="flir_lepton"
   pkg="flir_lepton"
   type="flir_lepton_node" >
  
   <param name="dataset/spline_interpolated_data" 
   value="/home/pandora/pandora_ws/src/rpi_hardware_interface/data/flir_lepton/dataset_spline_interp.pandora" />

  </node>

</launch>
